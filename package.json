{
  "name": "autodoc-ai",
  "version": "1.1.1",
  "type": "module",
  "description": "AI-Powered Project Documentation Generator using LLMs (Claude, OpenAI, Groq, Ollama)",
  "main": "dist/index.js",
  "bin": {
    "autodoc-ai": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "keywords": [
    "documentation",
    "ai",
    "llm",
    "generator",
    "cli",
    "autodoc",
    "typescript"
  ],
  "author": "Autodoc-AI Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@typescript-eslint/parser": "^8.56.1",
    "@typescript-eslint/typescript-estree": "^8.56.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "glob": "^13.0.6",
    "groq-sdk": "^0.37.0",
    "ignore": "^7.0.5",
    "inquirer": "^13.3.0",
    "openai": "^6.25.0"
  }
}